generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model students {
  id      Int    @id @default(autoincrement())
  name    String @db.VarChar(255)
  address String @db.VarChar(255)
}

model dosen {
  id      Int    @id @default(autoincrement())
  name    String @db.VarChar(255)
  address String @db.VarChar(255)
}
